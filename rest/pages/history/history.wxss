page {
  background-color: rgb(243, 243, 243);
}
.list{
  width: 100%;
  /* height: 20vh; */
  background-color: white;
  margin:2% 0%;
  border-radius: 16rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
 
}

.list image{
  width: 35%;
  margin-left: 3%;
}
.info{
  display: flex;
  flex-direction: column;
  justify-content: center;
  /* align-items: center; */
  width: 60%;
  margin-left: 5%;
}
.info .name{
  font-size: large;
  /* margin-top: 10rpx; */
  line-height: 200%;

}
.info text{
  margin-left: 16rpx;
  text-align: left;
}
.none{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100vh;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-size: 40rpx;
}
.none1{
  margin: 10% 0;
}
